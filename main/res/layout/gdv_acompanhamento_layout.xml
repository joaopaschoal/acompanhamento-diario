<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/gdv_acompanhamento_linear_layout"
    android:orientation="vertical"
    android:layout_marginTop="1dp">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:paddingLeft="5dp"
            android:paddingRight="5dp">
            <TextView android:id="@+id/gdv_acompanhamento_txv_data_acompanhamento"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dp"/>
        </LinearLayout>
        <View
            android:layout_width="1dip"
            android:layout_height="fill_parent"
            android:background="#cccccc" />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView android:id="@+id/gdv_acompanhamento_txv_tempo_aerobica"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/activity_default_left_margin" />
                <TextView android:id="@+id/gdv_acompanhamento_txv_tempo_musculacao"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/activity_default_left_margin" />
                <TextView android:id="@+id/gdv_acompanhamento_txv_tempo_total_estudo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/activity_default_left_margin" />
                <TextView android:id="@+id/gdv_acompanhamento_txv_tempo_total_trabalho"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/activity_default_left_margin" />
                <CheckBox android:id="@+id/gdv_acompanhamento_chk_exc_ur"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:button="@null"
                    android:drawableRight="?android:attr/listChoiceIndicatorMultiple"
                    android:text="ur:"
                    android:textSize="12dp"
                    android:textColor="#000"
                    android:layout_marginLeft="10dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginBottom="10dp">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="@dimen/activity_default_left_margin">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Assunto(s):"
                        android:textSize="12dp" />
                    <TextView android:id="@+id/gdv_acompanhamento_txv_assuntos_estudados"
                        android:layout_width="90dp"
                        android:background="@color/light_gray"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="@dimen/activity_default_left_margin">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Projeto(s):"
                        android:textSize="12dp" />
                    <TextView android:id="@+id/gdv_acompanhamento_txv_projetos_trabalhados"
                        android:layout_width="90dp"
                        android:background="@color/light_gray"
                        android:layout_height="wrap_content" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp">
        <View
            android:layout_width="fill_parent"
            android:layout_height="1dip"
            android:background="#cccccc" />
    </LinearLayout>
</LinearLayout>